{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "rootDir": "./src"
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.d.ts",
 
    ],
    "exclude": ["node_modules"]
  }
  